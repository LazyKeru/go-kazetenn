func GenerateSVGWeatherPictogram(width int, height int, chanceOfRain int, chanceOfSnow int, cloud int, IsSunUp int, IsMoonUp) string {
	res := "<svg xmlns='http://www.w3.org/2000/svg' width='" + width + "' height='" + height + "'>"
	res += "</svg>"
	return res
}

